.app-page-home{
  .banner{
    display flex
    align-items center
    gap 5em
    margin-top 5px
    img{
      height 30em
      width 70%
      object-fit cover
      border-radius 3px
    }
    &-text{
      position relative
      h1{
        font-size 100px
      }
      .p-tag{
        font-size 20px
        position absolute
        top -10px
        right 0
      }
    }
  }
}